#include <stdio.h>
#include <stdlib.h>


int argc = 6;

int main(int argc, char *argv[]) {
	printf("start");
	for(int i = 1; i<argc; i++){
		printf("-------");
		printf("%s%s", argv[i], (i<argc-1)?" ":"");
	}
	printf("\n");
	return 0;
}
